digraph FA1 {
	"{X}" [shape=doublecircle]
	"{S}" [shape=circle]
	"{D}" [shape=circle]
	"{F}" [shape=circle]
	"" [shape=plaintext]
	"" -> "{S}" [label=start]
	"{S}" -> "{F}" [label=a]
	"{S}" -> "{S}" [label=b]
	"{F}" -> "{F}" [label=b]
	"{F}" -> "{D}" [label=c]
	"{F}" -> "{X}" [label=a]
	"{D}" -> "{S}" [label=c]
	"{D}" -> "{X}" [label=a]
}
