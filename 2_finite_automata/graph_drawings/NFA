digraph NFA {
	"{C}" [shape=doublecircle]
	"{D}" [shape=circle]
	"{A}" [shape=circle]
	"{B}" [shape=circle]
	"" [shape=plaintext]
	"" -> "{A}" [label=start]
	"{A}" -> "{B}" [label=a]
	"{A}" -> "{A}" [label=b]
	"{B}" -> "{C}" [label=a]
	"{B}" -> "{B}" [label=c]
	"{C}" -> "{D}" [label=a]
	"{D}" -> "{D}" [label=a]
	"{D}" -> "{B}" [label=a]
}
